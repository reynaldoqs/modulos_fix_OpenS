               v-model=\"selectedPerfil.nombre\"\n                :disabled=\"!editable\"\n                inverted\n                color=\"tertiary\"\n                placeholder=\"Nombre\"\n                @keyup.enter=\"patchPerfil\"\n              />\n            </div>\n            <div class=\"col-8\">\n              <q-input\n                v-model=\"selectedPerfil.descripcion\"\n                :disabled=\"!editable\"\n                inverted\n                color=\"tertiary\"\n                placeholder=\"DescripciÃ³n\"\n                @keyup.enter=\"patchPerfil\"\n              />\n            </div>\n          </div>\n          <div class=\"botones-container\">\n            <div class=\"col-6\">\n              <q-btn @click=\"cancel\" color=\"neutral\" size=\"md\" label=\"Cancelar\"/>\n            </div>\n            <div class=\"col-6\">\n              <q-btn\n                :disabled=\"!editable\"\n                @click=\"patchPerfil\"\n                color=\"positive\"\n                size=\"md\"\n                label=\"Guardar\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport PerfilesAvatarsList from \"./AvatarsList\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  data: () => ({\n    editable: true,\n    idPerfil: \"\",\n    selectedPerfil: {\n      nombre: \"\",\n      descripcion: \"\",\n      urlImagen: \"\"\n    }\n  }),\n  methods: {\n    patchPerfil() {\n      if (this.isAuthenticated) {\n        this.$store\n          .dispatch(\"patchPerfil\", {\n            clientId: this.currentUser.id,\n            idPerfil: this.idPerfil,\n            nombre: this.selectedPerfil.nombre,\n            descripcion: this.selectedPerfil.descripcion,\n            urlImagen: this.selectedPerfil.urlImagen.toString()\n          })\n          .then(data => {\n            delete this.$route.query.perfiles;\n            this.$router.push({\n              path: \"perfil\",\n              query: { perfil: \"selection\" }\n            });\n          })\n          .catch(err => {\n            this.$q.notify({\n              message: err,\n              type: \"negative\",\n              color: \"negative\",\n              textColor: \"white\",\n              icon: \"error\",\n              position: \"top-right\"\n            });\n          });\n      }\n    },\n    cancel() {\n      this.$router.go(-1);\n    }\n  },\n  computed: {\n    ...mapGetters([\"userPerfiles\", \"isAuthenticated\", \"currentUser\"])\n  },\n  mounted() {\n    this.idPerfil = this.$route.query.id || \"\";\n    let perfilVuex = this.userPerfiles.find(data => data.id === this.idPerfil);\n    console.log(perfilVuex);\n    this.selectedPerfil = JSON.parse(JSON.stringify(perfilVuex));\n  },\n  components: {\n    PerfilesAvatarsList\n  }\n};\n</script>\n<style scoped>\n.creation-btn {\n  width: 100%;\n}\n.perfiles-container {\n  max-width: 500px;\n  margin: 0 auto;\n  background-color: var(--bg-content);\n  padding: 14px;\n}\n.perfil-form {\n  padding-top: 20px;\n}\n.botones-container {\n  display: flex;\n  justify-content: flex-end;\n  position: relative;\n  margin-top: 10px;\n  top: 16px;\n  right: -16px;\n}\n.perfil-selection-title {\n  text-align: center;\n  padding: 2px 10px 30px 10px;\n}\n.perfil-selection-title > p {\n  color: var(--text-small);\n  margin-top: 2px;\n  margin-bottom: 0;\n}\n.perfil-selection-title > h3 {\n  font-size: 1.5em;\n  font-weight: 400;\n}\n</style>\n"],"sourceRoot":"src/Usuario/components/perfil"}]}