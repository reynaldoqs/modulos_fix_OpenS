d\">\n      <span class=\"title\">Copiar:</span>\n      <q-btn\n        @click=\"copyClip('token')\"\n        :loading=\"loading\"\n        label=\"TOKEN\"\n        style=\"margin-right:1px\"\n        color=\"info\"\n        size=\"sm\"\n      />\n      <q-btn\n        @click=\"copyClip('url')\"\n        :loading=\"loading\"\n        label=\"URL\"\n        style=\"margin-right:1px\"\n        color=\"info\"\n        size=\"sm\"\n      />\n      <q-btn\n        @click=\"copyClip('curl')\"\n        :loading=\"loading\"\n        label=\"CURL\"\n        style=\"margin-right:1px\"\n        color=\"info\"\n        size=\"sm\"\n      />\n    </div>\n    <div class=\"iop-request\">\n      <div>\n        <span class=\"color-1\">curl</span>\n        <span class=\"e-char\">-</span>\n        X {{data.data.metodo}} \\\n      </div>\n      <div class=\"tab\">\n        <span class=\"url\">{{data.data.ruta}}</span> \\\n      </div>\n      <div class=\"tab\">\n        <span class=\"e-char\">-</span>H\n        <span class=\"headers simply\">Content-Type: application/json</span> \\\n      </div>\n      <div class=\"tab\">\n        <span class=\"e-char\">-</span>H\n        <span class=\"headers simply\">Authorization: {{data.data.token}}</span>\\\n      </div>\n      <div class=\"tab\">\n        <span class=\"e-char\">-</span>d\n        <span class=\"simply\">\n          <span class=\"color-1\">{</span>\n          <div class=\"body-element tab\" v-for=\"(element, index) in body\" :key=\"index\">\n            <span class=\"key double\">{{element.key}}</span> :\n            <span\n              :class=\"{'value':true,double:typeof element.value !=='number'}\"\n            >{{element.value}}</span>\n          </div>\n          <span class=\"color-1\">}</span>\n        </span>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"iop-request\",\n  data: () => ({\n    loading: false\n  }),\n  props: {\n    data: Object\n  },\n  methods: {\n    copyClip(type) {\n      this.loading = true;\n      const { ruta, token, metodo, body } = this.data.data;\n      let text = \"\";\n\n      switch (type) {\n        case \"curl\":\n          text = `curl -X ${metodo} ${ruta} -H 'Content-Type: application/json' -H 'Authorization: ${token}' -d '${JSON.stringify(\n            body\n          ).toString()}'`;\n          break;\n        case \"token\":\n          text = `${token}`;\n          break;\n        case \"url\":\n          text = `${ruta}`;\n          break;\n        default:\n          text = `curl -X ${metodo} ${ruta} -H 'Content-Type: application/json' -H 'Authorization: ${token}' -d '${JSON.stringify(\n            body\n          ).toString()}'`;\n          break;\n      }\n\n      if (!navigator.clipboard) {\n        this.oldCopy(text);\n        return;\n      }\n      navigator.clipboard\n        .writeText(text)\n        .then(() => {\n          this.loading = false;\n        })\n        .catch(err => console.error(err));\n    },\n    oldCopy(data) {\n      var textArea = document.createElement(\"textarea\");\n      textArea.value = data;\n      document.body.appendChild(textArea);\n      textArea.focus();\n      textArea.select();\n      try {\n        var successful = document.execCommand(\"copy\");\n        this.loading = false;\n      } catch (err) {\n        console.error(err);\n      }\n\n      document.body.removeChild(textArea);\n    }\n  },\n  computed: {\n    body() {\n      if (this.data.data.body) {\n        return Object.entries(this.data.data.body).map(data => ({\n          key: data[0],\n          value: data[1]\n        }));\n      }\n      return [];\n    }\n  }\n};\n</script>\n<style scoped>\n.iop-request {\n  user-select: none;\n  background-color: var(--bg-inputs);\n  padding: 10px;\n  font-size: 12px;\n  font-family: \"Courier New\", Courier, monospace;\n  color: var(--text-color);\n  width: 100%;\n  max-width: 100%;\n  overflow-x: auto;\n}\n.copy-container {\n  position: relative;\n}\n.copy-clipboard {\n  position: absolute;\n  right: 0px;\n}\n.tab {\n  margin-left: 10px;\n}\n.e-char {\n  color: var(--red);\n  margin: 0 2px 0 4px;\n}\n.color-1 {\n  color: var(--text-color);\n}\n.headers {\n  color: #ffe0b2;\n}\n.simply::after,\n.simply::before {\n  content: \"'\";\n  color: var(--red);\n}\n.double::after,\n.double::before {\n  content: '\"';\n}\n.key {\n  color: #dcedc8;\n}\n.value {\n  color: #b2ebf2;\n}\n.url {\n  font-weight: 700;\n}\n.title {\n  font-size: 12px;\n  margin-right: 10px;\n  color: white;\n}\n</style>\n"],"sourceRoot":"src/shared/components"}]}