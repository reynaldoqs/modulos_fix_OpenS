tton\"\n            size=\"sm\"\n            color=\"positive\"\n            glossy\n          >INGRESAR</q-btn>\n        </div>\n        <a href=\"#\">¿Olvidaste tu contraseña?</a>\n        <a @click.prevent=\"openRegistro\" href=\"#\">Registrarse</a>\n      </section>\n      <footer class=\"login-footer\">\n        <iop-footer/>\n      </footer>\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      editable: true,\n      usuario: \"\",\n      clave: \"\"\n    };\n  },\n  computed: {\n    isValid() {\n      return this.usuario.length > 0 && this.clave.length > 0;\n    }\n  },\n  methods: {\n    openRegistro() {\n      this.$hub.$emit(\"open-modal\");\n      this.$hub.$emit(\"set-modal-data\", {}, \"Registro\", \"success\");\n    },\n    login() {\n      this.editable = false;\n      this.$store\n        .dispatch(\"login\", { usuario: this.usuario, clave: this.clave })\n        .then(data => {\n          /* this.$q.notify({\n            message: \"Datos correctos, ingreso exitoso\",\n            type: \"positive\",\n            color: \"positive\",\n            textColor: \"white\",\n            icon: \"verified_user\",\n            position: \"top-right\"\n          }); */\n\n          setTimeout(() => {\n            this.$router.push({\n              name: \"perfil\",\n              query: { perfil: \"selection\" }\n            });\n            this.editable = true;\n          }, 200);\n        })\n        .catch(err => {\n          this.$q.notify({\n            message: `Error en el login: ${err}`,\n            type: \"negative\",\n            color: \"negative\",\n            textColor: \"white\",\n            icon: \"error\",\n            position: \"top-right\"\n          });\n          this.editable = true;\n        });\n    }\n  }\n};\n</script>\n<style>\n.lg-button {\n  margin-top: 5px;\n  width: 100%;\n}\n\n.login-input {\n  margin: 20px 0;\n}\n.login-bg {\n  width: 100%;\n  height: 100vh;\n  background-image: url(../../assets/bg.jpg);\n  background-size: cover;\n  -webkit-filter: blur(2px);\n  filter: blur(7px);\n}\n.login {\n  width: 100%;\n  height: 100%;\n  background-color: rgba(25, 22, 41, 0.86);\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n  position: absolute;\n  top: 0;\n  left: 0;\n}\nsection.login-container {\n  background-color: var(--bg-content);\n  width: 320px;\n  margin: 70px 0 0 0;\n  text-align: center;\n  padding: 60px 25px 25px 25px;\n  border-radius: 2px;\n  color: silver;\n}\nimg {\n  max-width: 180px;\n  margin-bottom: 20px;\n}\n\na {\n  display: block;\n  color: silver;\n  font-size: 0.85em;\n  margin: 30px 0 10px 0;\n}\na:last-child {\n  margin-top: 10px;\n  margin-bottom: 10px;\n}\nfooter.login-footer {\n  width: 100%;\n  height: var(--footer-height);\n}\n@media (max-width: 768px) {\n  section.login-container {\n    margin: 0;\n    width: 100%;\n    height: 100%;\n  }\n  .login {\n    width: 100%;\n    height: 100vh;\n  }\n}\n</style>\n"],"sourceRoot":"src/Visitante/views"}]}