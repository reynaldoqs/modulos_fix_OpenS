;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4GA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Selection.vue","sourceRoot":"src/Usuario/components/perfil","sourcesContent":["<template>\n  <div class=\"perfil-selection\">\n    <header class=\"perfil-selection-title\">\n      <h3>Selección de perfil</h3>\n      <p>\n        El perfil estará asociado a todos los consumos que realize,\n        {{userPerfiles.length > 0? 'seleccione un perfil para continuar':'debe agregar un perfil para continuar'}}\n      </p>\n    </header>\n    <div class=\"selector\">\n      <perfil-avatar\n        v-for=\"perfil in userPerfiles\"\n        :key=\"perfil.id\"\n        :nombre=\"perfil.nombre\"\n        :urlImagen=\"perfil.urlImagen\"\n        :descripcion=\"perfil.descripcion\"\n        :estado=\"perfil.estado\"\n        :esProduccion=\"perfil.esProduccion\"\n        :idPerfil=\"perfil.id\"\n        :isSelected=\"false\"\n        @onSelect=\"perfil.esProduccion?showPerfil(perfil.id):selectPerfil(perfil.id)\"\n        @onDelete=\"deletePerfil(perfil.id)\"\n        @onEdit=\"editPerfil(perfil.id)\"\n      />\n      <perfil-add @onAdd=\"$emit('onAdd')\"/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport PerfilAvatar from \"./PerfilAvatar\";\nimport PerfilAdd from \"./PerfilAdd\";\nexport default {\n  name: \"perfiles-selection\",\n  components: {\n    PerfilAvatar,\n    PerfilAdd\n  },\n  methods: {\n    showPerfil(idPerfil) {\n      console.log(\"detalles del perfil after-production\");\n    },\n    selectPerfil(idPerfil) {\n      this.$store.dispatch(\"changePerfil\", idPerfil);\n      if (this.isAuthenticated && this.hasPerfil) {\n        this.$router.push({ name: \"composicion\" });\n      }\n    },\n    deletePerfil(idPerfil) {\n      if (this.isAuthenticated) {\n        this.$q\n          .dialog({\n            title: \"Confirmar\",\n            message: \"Esta apunto de eliminar un perfil para siempre\",\n            ok: \"Eliminar\",\n            color: \"grey\",\n            cancel: \"Cancelar\"\n          })\n          .then(() => {\n            this.$store\n              .dispatch(\"removePerfil\", {\n                clientId: this.currentUser.id,\n                idPerfil: idPerfil\n              })\n              .then(() => {\n                this.$q.notify({\n                  message: \"Perfil eliminado con extito\",\n                  type: \"positive\",\n                  color: \"positive\",\n                  textColor: \"white\",\n                  icon: \"check_circle\",\n                  position: \"top-right\"\n                });\n              })\n              .catch(err => {\n                this.$q.notify({\n                  message: err,\n                  type: \"negative\",\n                  color: \"negative\",\n                  textColor: \"white\",\n                  icon: \"error\",\n                  position: \"top-right\"\n                });\n              });\n          });\n      } else {\n        console.error(\"not authenticated\");\n      }\n    },\n    editPerfil(idPerfil) {\n      this.$router.push({\n        query: { perfil: \"edit\", id: idPerfil }\n      });\n    }\n  },\n  computed: {\n    ...mapGetters([\n      \"userPerfiles\",\n      \"hasPerfil\",\n      \"isAuthenticated\",\n      \"currentPerfil\",\n      \"currentUser\"\n    ])\n  }\n};\n</script>\n<style scoped>\n.selector {\n  width: 100%;\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n}\n.perfil-selection-title {\n  text-align: center;\n  padding: 2px 10px 30px 10px;\n}\n.perfil-selection-title > p {\n  color: var(--text-small);\n  margin-top: 2px;\n  margin-bottom: 0;\n}\n.perfil-selection-title > h3 {\n  font-size: 1.5em;\n  font-weight: 400;\n}\n</style>\n"]}]}