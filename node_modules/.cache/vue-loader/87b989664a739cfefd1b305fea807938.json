list :composiciones=\"composiciones\" @onSelect=\"seleccionarComposicion\"/>\n  </div>\n</template>\n<script>\nimport CompocicionesList from \"./ComposicionesList\";\nimport { composicionApi } from \"@/services\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"composiciones\",\n  data: () => ({\n    composiciones: []\n  }),\n  methods: {\n    seleccionarComposicion(idComposicion) {\n      const data = {\n        idPerfil: this.currentPerfil.id,\n        idComposicion\n      };\n      composicionApi\n        .solicitarCredenciales(data)\n        .then(data => {\n          this.$hub.$emit(\"open-modal\");\n          this.$hub.$emit(\"set-modal-data\", data, \"Credenciales\", \"success\");\n        })\n        .catch(err => {\n          this.$q.notify({\n            message: `${err.data.message}`,\n            type: \"negative\",\n            color: \"negative\",\n            textColor: \"white\",\n            icon: \"error\",\n            position: \"top-right\"\n          });\n        });\n    },\n    loadComposiciones(idPerfil) {\n      composicionApi\n        .listarComposiciones(idPerfil)\n        .then(data => {\n          this.composiciones = data.data;\n        })\n        .catch(err => {\n          alert(err);\n        });\n    }\n  },\n  mounted() {\n    this.loadComposiciones(this.currentPerfil.id);\n  },\n  components: {\n    CompocicionesList\n  },\n  computed: {\n    ...mapGetters([\"currentUser\", \"currentPerfil\"])\n  }\n};\n</script>\n<style scoped>\n.composiciones-container {\n  width: 100%;\n  height: 100%;\n  max-height: 100%;\n  overflow-y: auto;\n  font-size: 16px;\n}\n</style>\n"]}]}