\"curl\":\n          text = `curl -X ${metodo} ${ruta} -H 'Content-Type: application/json' -H 'Authorization: ${token}' -d '${JSON.stringify(\n            body\n          ).toString()}'`;\n          break;\n        case \"token\":\n          text = `${token}`;\n          break;\n        case \"url\":\n          text = `${ruta}`;\n          break;\n        default:\n          text = `curl -X ${metodo} ${ruta} -H 'Content-Type: application/json' -H 'Authorization: ${token}' -d '${JSON.stringify(\n            body\n          ).toString()}'`;\n          break;\n      }\n\n      if (!navigator.clipboard) {\n        this.oldCopy(text);\n        return;\n      }\n      navigator.clipboard\n        .writeText(text)\n        .then(() => {\n          this.loading = false;\n        })\n        .catch(err => console.error(err));\n    },\n    oldCopy(data) {\n      var textArea = document.createElement(\"textarea\");\n      textArea.value = data;\n      document.body.appendChild(textArea);\n      textArea.focus();\n      textArea.select();\n      try {\n        var successful = document.execCommand(\"copy\");\n        this.loading = false;\n      } catch (err) {\n        console.error(err);\n      }\n\n      document.body.removeChild(textArea);\n    }\n  },\n  computed: {\n    body() {\n      if (this.data.data.body) {\n        return Object.entries(this.data.data.body).map(data => ({\n          key: data[0],\n          value: data[1]\n        }));\n      }\n      return [];\n    }\n  }\n};\n</script>\n<style scoped>\n.iop-request {\n  user-select: none;\n  background-color: var(--bg-inputs);\n  padding: 10px;\n  font-size: 12px;\n  font-family: \"Courier New\", Courier, monospace;\n  color: var(--text-color);\n  width: 100%;\n  max-width: 100%;\n  overflow-x: auto;\n}\n.copy-container {\n  position: relative;\n}\n.copy-clipboard {\n  position: absolute;\n  right: 0px;\n}\n.tab {\n  margin-left: 10px;\n}\n.e-char {\n  color: var(--red);\n  margin: 0 2px 0 4px;\n}\n.color-1 {\n  